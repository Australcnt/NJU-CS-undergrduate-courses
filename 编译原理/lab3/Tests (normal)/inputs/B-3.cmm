int main() {
  int idx, s, e;
  int tmp1, tmp2;
  int len = 5;
  int a[5];
  int cnt = 0;
  int stop = 0;
  while (cnt < len) {
    a[cnt] = read();
    cnt = cnt + 1;
  }

  cnt = len / 2 - 1;
  while (cnt >= 0) {
    s = cnt;
    e = len;
    tmp1 = a[s];
    stop = 0;
    while ((s * 2 + 1 < e) && !stop) {
      idx = s * 2 + 1;
      if ((idx + 1 < e) && a[idx + 1] > a[idx]) {
        idx = idx + 1;
      }
      if (a[idx] > tmp1) {
        a[s] = a[idx];
        s = idx;
      } else {
        stop = 1;
      }
    }
    a[s] = tmp1;
    cnt = cnt - 1;
  }

  cnt = len - 1;
  while (cnt >= 0) {
    tmp2 = a[0];
    a[0] = a[cnt];
    a[cnt] = tmp2;
    s = 0;
    e = cnt;
    tmp1 = a[s];
    stop = 0;
    while ((s * 2 + 1 < e) && !stop) {
      idx = s * 2 + 1;
      if ((idx + 1 < e) && a[idx + 1] > a[idx]) {
        idx = idx + 1;
      }
      if (a[idx] > tmp1) {
        a[s] = a[idx];
        s = idx;
      } else {
        stop = 1;
      }
    }
    a[s] = tmp1;
    cnt = cnt - 1;
  }

  cnt = 0;
  while (cnt < len) {
    write(a[cnt]);
    cnt = cnt + 1;
  }
  return 0;
}
