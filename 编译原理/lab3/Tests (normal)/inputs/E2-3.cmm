int get_max(int max_arr[4]) {
  int max_idx = 0;
  int max_val = max_arr[0];

  int max_len = 4;
  int max_cnt = 0;
  while (max_cnt < max_len) {
    if (max_arr[max_cnt] > max_val) {
      max_idx = max_cnt;
      max_val = max_arr[max_cnt];
    }
    max_cnt = max_cnt + 1;
  }
  return max_idx;
}

int mod(int m1, int m2) {
  return m1 - (m1 / m2) * m2;
}

int main() {
  int len = 4;
  int val_arr[4][4];
  int res_arr[4];
  int copy_arr[2];
  int o_cnt = 0;
  int i_cnt = 0;

  o_cnt = 0;
  while (o_cnt < len) {
    i_cnt = 0;
    while (i_cnt < len) {
      val_arr[o_cnt][i_cnt] = mod(o_cnt + i_cnt, len);
      i_cnt = i_cnt + 1;
    }
    res_arr[o_cnt] = get_max(val_arr[o_cnt]);
    o_cnt = o_cnt + 1;
  }

  copy_arr = res_arr;
  write(copy_arr[0]);
  write(copy_arr[1]);
  return 0;
}
